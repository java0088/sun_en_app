/* 小标签 */
.tag {
  /* display: inline-block; */
  padding: 4rpx 8rpx;
  border-radius: 6rpx;
  background-color: #6b4cd6;
  margin-right: 12rpx;
}
.tit {
  font-size: 30rpx;
  color: #222;
  font-weight: 700;
}
/* 用户头像 */
.avatar {
  position: relative;
  width: 40px;
  height: 40px;
  border-radius: 50%;
  margin-top: 12rpx;
  background-color: #eee9ef;
}
.avatar .iconfont {
  position: absolute;
  color: #fff;
  font-weight: 700;
  font-size: 13px;
}
.avatar .day {
  position: absolute;
  right: -40%;
  bottom: -10rpx;
  background-color: #fc3530;
  color: #fff;
  font-size: 10rpx;
  padding: 0px 4rpx;
}
.cate_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 28rpx;
  background-color: #fff;
}
.cate_box .cate_item {
  position: relative;
  padding: 30rpx;
  color: #94959a;
  font-weight: 700;
}
.cate_box .cate_item::after {
  position: absolute;
  content: '';
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  right: 0;
  width: 2rpx;
  height: 26rpx;
  border-radius: 2rpx;
  background-color: #d6d7d9;
}
.cate_box .cate_item:nth-child(3)::after {
  background-color: rgba(0, 0, 0, 0);
}
.cate_box .active {
  color: #222;
}
.v_item {
  position: relative;
  width: 31%;
  height: 260rpx;
  border-radius: 12rpx;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.v_item .item_src {
  width: 100%;
  height: 100%;
}
.v_item .play_btn {
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -50%);
          transform: translate(-50%, -50%);
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, 0.3);
}
.v_item .play_btn .iconfont {
  font-size: 20rpx;
  color: #fff;
}
.v_item .love {
  position: absolute;
  bottom: 12rpx;
  left: 12rpx;
  color: #fff;
}
.v_item .love .iconfont {
  font-size: 28rpx;
}
.v_item .love .num {
  font-size: 22rpx;
  margin-left: 8rpx;
  font-weight: normal;
}
.new_tag {
  position: absolute;
  top: 0;
  right: 0;
  border-radius: 50% 0 50% 0;
  background-color: #da4a27;
  color: #fff;
  padding: 3rpx 8rpx;
  font-size: 22rpx;
}

